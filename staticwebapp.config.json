{
  "routes": [
    {
      "route": "/privacy.html",
      "rewrite": "/privacy.html"
    },
    {
      "route": "/terms.html",
      "rewrite": "/terms.html"
    },
    {
      "route": "/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "methods": ["GET"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/images/*", "/*.{css,js,json,png,jpg,jpeg,gif,svg,ico,webp,woff,woff2,ttf,eot}"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "mimeTypes": {
    ".webmanifest": "application/manifest+json",
    ".json": "application/json"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
